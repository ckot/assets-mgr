{
  "name": "@assets-mgr/media-config",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "media-config",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/media-config/dist",
          "main": "libs/media-config/src/index.ts",
          "tsConfig": "libs/media-config/tsconfig.lib.json",
          "format": [
            "esm"
          ],
          "declarationRootDir": "libs/media-config/src"
        }
      }
    }
  },
  "dependencies": {}
}
